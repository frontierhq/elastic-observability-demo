{
  "index_patterns": [
    "logs-*-*"
  ],
  "composed_of": [
    "frontier-logs@mappings",
    "frontier-logs@settings",
    "ecs-base@8.11.0",
    "ecs-data_stream@8.11.0",
    "ecs-ecs@8.11.0",
    "ecs@mappings"
  ],
  "priority": 200,
  "data_stream": {
    "hidden": false,
    "allow_custom_routing": false
  },
  "template": {
    "mappings": {
      "date_detection": false
    },
    "settings": {
      "index": {
        "codec": "best_compression",
        "lifecycle": {
          "name": "frontier-logs"
        },
        "default_pipeline": "frontier-logs@default"
      }
    }
  }
}
