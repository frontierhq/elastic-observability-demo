{
  "index_patterns": [
    "logs-rancher.lima-*"
  ],
  "composed_of": [
    "frontier-ecs-base@8.11.0",
    "frontier-ecs-data_stream@8.11.0",
    "frontier-ecs-ecs@8.11.0",
    "ecs@mappings"
  ],
  "priority": 300,
  "data_stream": {
    "hidden": false,
    "allow_custom_routing": false
  },
  "template": {
    "mappings": {
      "date_detection": false
    },
    "settings": {
      "index": {
        "codec": "best_compression",
        "lifecycle": {
          "name": "frontier-logs"
        },
        "default_pipeline": "frontier-logs-rancher.lima@default"
      }
    }
  }
}
