{
  "description": "Default ingest pipeline for k3s logs.",
  "processors": [
    {
      "append": {
        "field": "tags",
        "value": [
          "frontier-logs-k3s@default"
        ]
      }
    }
  ],
  "on_failure": [
    {
      "set": {
        "field": "error.message",
        "value": "{{ _ingest.on_failure_message }}"
      }
    }
  ]
}
