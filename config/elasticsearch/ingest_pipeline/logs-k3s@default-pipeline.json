{
  "description": "Default ingest pipeline for k3s logs.",
  "processors": [
    {
      "append": {
        "field": "tags",
        "value": [
          "logs-k3s@default-pipeline"
        ]
      }
    },
    {
      "pipeline": {
        "description": "Run logs common default pipeline.",
        "name": "logs-common@default-pipeline"
      }
    }
  ],
  "on_failure": [
    {
      "set": {
        "field": "error.message",
        "value": "{{ _ingest.on_failure_message }}"
      }
    }
  ]
}
