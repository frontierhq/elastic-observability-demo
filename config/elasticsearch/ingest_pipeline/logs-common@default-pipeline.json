{
  "description": "Default ingest pipeline for all logs.",
  "on_failure": [
    {
      "set": {
        "field": "error.message",
        "value": "{{ _ingest.on_failure_message }}"
      }
    }
  ],
  "processors": [
    {
      "append": {
        "field": "tags",
        "value": [
          "logs-common@default-pipeline"
        ]
      }
    },
    {
      "set": {
        "field": "ecs.version",
        "if": "ctx.ecs?.version == null",
        "value": "8.11.0"
      }
    },
    {
      "set": {
        "copy_from": "message",
        "field": "event.original",
        "if": "ctx.event?.original == null"
      }
    },
    {
      "set": {
        "copy_from": "event.dataset",
        "field": "data_stream.dataset",
        "if": "ctx.data_stream?.dataset == null"
      }
    },
    {
      "set": {
        "field": "event.ingested",
        "if": "ctx.event?.ingested == null",
        "value": "{{_ingest.timestamp}}"
      }
    }
  ]
}
